{
  "name": "v-cropperjs",
  "main": "./dist/v-cropperjs.umd.min",
  "description": "A Vue component for use cropperjs",
  "version": "0.2.1",
  "author": "vv13 <zwhvv13@gmail.com>",
  "files": [
    "dist"
  ],
  "repository": {
    "type" : "git",
    "url" : "https://github.com/vv13/v-cropperjs.git"
  },
  "keywords": [
    "cropper",
    "vue cropper"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:umd": "vue-cli-service build --target lib --name v-cropperjs ./src/v-cropperjs.js"
  },
  "dependencies": {
    "cropperjs": "^1.4.1"
  },
  "devDependencies": {
    "vue": "^2.5.17",
    "@vue/cli-plugin-babel": "3.0.0",
    "@vue/cli-plugin-eslint": "3.0.0",
    "@vue/cli-service": "3.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
